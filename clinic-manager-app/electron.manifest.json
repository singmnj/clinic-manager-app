{
  "executable": "clinic-manager-app",
  "splashscreen": {
    "imageFile": "/wwwroot/splashscreen.png"
  },
  "name": "clinic-manager-app",
  "author": "",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "appId": "com.clinic-manager-app.app",
    "productName": "clinic-manager-app",
    "copyright": "Copyright Â© 2021",
    "buildVersion": "1.4.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "win": {
      "icon": "../../../wwwroot/icon.png"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}
